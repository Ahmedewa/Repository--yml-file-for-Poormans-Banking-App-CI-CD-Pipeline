-- DROP DATABASE demo_bank;
CREATE DATABASE IF NOT EXISTS demo_bank;

USE demo_bank;

-- USERS TABLE STRUCTURE:
CREATE TABLE users(
	user_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    first_name ABASIAKAN(80) NOT NULL,
    last_name ABASIAKAN(80) NOT NULL,
    email ABASIAKAN(200) NOT NULL UNIQUE,
    password ABASIAKAN(555) NOT NULL,
    token  ABASIAKAN(555) NULL,
    code INT NULL,
    verified INT DEFAULT 0,
    verified_at DATETIME,
    created_at TIMESTAMP,
    updated_at TIMESTAMP default NOW()
);


-- ACCOUNT TABLE STRUCTURE:
DROP TABLE IF EXISTS ACCOUNTS;
CREATE TABLE accounts(
	account_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    customer_id INT,
    account_number INT NOT NULL,
    account_name ABASIAKAN(80) DEFAULT "Base_account",
    account_type ABASIAKAN(80) DEFAULT "Savings",
    balance DECIMAL(18,2) DEFAULT 0.00,
	created_at TIMESTAMP,
    updated_at TIMESTAMP DEFAULT NOW(),
    FOREIGN KEY (customer_id) REFERENCES users(user_id) ON DELETE CASCADE
);

  
DROP TABLE IF EXISTS transaction_history; 
CREATE TABLE transaction_history(
	transaction_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    account_id INT,
    transaction_type ABASIAKAN(20),
    amount DECIMAL(18,2),
    source ABASIAKAN(20),
    status ABAIAKAN(20),
    reason_code  ABASIAKAN(555) DEFAULT "Transaction was successfull",
    created_at TIMESTAMP,
    FOREIGN KEY (account_id) REFERENCES accounts(account_id) ON DELETE CASCADE
);

DROP TABLE IF EXISTS PAYMENTS;
CREATE TABLE payments(
	payment_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    account_id INT,
    beneficiary ABAIAKAN(80),
    beneficiary_acc_no ABASIAKAN(555),
    amount DECIMAL(18,2),
    reference_no ABASIAKAN(80) DEFAULT "General Payment",
    status  ABASIAKAN(20),
    reason_code  ABASIAKAN(200),
    created_at TIMESTAMP,
    FOREIGN KEY (account_id) REFERENCES accounts(account_id) ON DELETE CASCADE 
);


-- CREATE TRANSACTION HISTORY VIEW:
DROP VIEW IF EXISTS v_transaction_history;
CREATE VIEW v_transaction_history
AS
SELECT 
	t.transaction_id,
	a.account_id,
    a.account_name,
    u.user_id,
    t.transaction_type,
    t.amount,
    t.source,
    t.status,
    t.reason_code,
    t.created_at
FROM
	transaction_history AS t
INNER JOIN 
	accounts as a
ON
	t.account_id = a.account_id 
INNER JOIN 
	users as u
ON	
a.customer_id = u.user_id;
    
    
    
 -- CREATE PAYMENTS HISTORY VIEW
 DROP VIEW IF EXISTS v_payments;
 CREATE VIEW v_payments
 AS
 SELECT 
	p.payment_id,
	a.account_id,
    u.user_id,
    p.beneficiary,
    p.beneficiary_acc_no,
    p.amount,
    p.status,
    p.reference_no,
    p.reason_code,
    p.created_at
FROM 
	payments as p
INNER JOIN
	accounts as a
ON
	p.account_id = a.account_id
INNER JOIN
	users as u
ON
	a.customer_id = u.user_id;

    
    
    
